[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
x509_extensions    = v3_req
distinguished_name = dn

[dn]
CN = development.local

[v3_req]
subjectAltName = @alt_names
keyUsage = keyEncipherment, dataEncipherment
extendedKeyUsage = serverAuth

[alt_names]
DNS.1 = localhost
DNS.2 = development.local
IP.1 = 127.0.0.1
IP.2 = ::1
IP.3 = 192.168.20.185
IP.4 = 192.168.134.47
IP.5 = 192.168.1.3
IP.6 = 192.168.1.4
IP.7 = 192.168.1.5
IP.8 = 10.0.0.1
IP.9 = 10.0.0.2
